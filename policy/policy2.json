{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetBucketVersioning",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::terminal"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:ReplicateTags",
        "s3:GetObject",
        "s3:GetObjectVersion",
        "s3:GetObjectVersionTagging",
        "s3:PutObject",
        "s3:ReplicateObject"
      ],
      "Resource": [
        "arn:aws:s3:::terminal/*"
      ]
    }
  ]
}